<!--
<a href="javascript:ShowAll();">show all</a> &#151;
<a href="javascript:HideIfEmpty();">hide if empty</a>
-->

<script>
function ShowAll(){
  $(".composer").each(function(){$(this).show();});
  $(".title").each(function(){$(this).show();});
}

function HideIfEmpty(){
  $(".composer").each(function(){
    if($(this).text().length<1){$(this).remove();}
  });
  $(".title").each(function(){
    if($(this).text().length<1){$(this).remove();}
  });
}

</script>

